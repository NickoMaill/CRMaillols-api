{
  "name": "crmaillols-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production && node dist/src/app.js",
    "docker": "docker-compose --env-file .env.development.local up -d",
    "dev": "NODE_ENV=development.local nodemon --watch src -e js,ts,json --exec \"ts-node src/app.ts\"",
    "copy-file": "cp -R public dist/ && cp -R src/views dist/src/",
    "build": "rm -r dist/ && tsup src/app.ts migrations --format cjs --clean && npm run copy-file",
    "test": "echo \"Error: no test specified\" && exit 1",
    "sequelize": "NODE_ENV=development sequelize-cli --env development",
    "sequelize-init": "npm run sequelize init && npx tsc --project .sequelizetsconfig.json",
    "update-sequelize": "npx tsc --project .sequelizetsconfig.json",
    "init-server": "npm i && npm run sequelize-init ; npm run docker && npm run sequelize db:migrate && npm run sequelize db:migrate:status"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/express-rate-limit": "^6.0.0",
    "@types/http-errors": "^1.8.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.6",
    "@types/node-fetch": "^2.6.2",
    "@types/winston": "^2.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-list-endpoints": "^6.0.0",
    "express-rate-limit": "^6.5.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "jspdf": "^2.5.1",
    "limiter": "^2.1.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.2.10",
    "node-schedule": "^2.1.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.4",
    "sequelize-cli": "^6.4.1",
    "sib-api-v3-sdk": "^8.4.2",
    "sib-api-v3-typescript": "^2.2.2",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
    "tsup": "^6.2.2",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "xss": "^1.0.14"
  }
}
